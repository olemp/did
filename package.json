    "scripts": {
        "start": "node ./bin/www",
        "watch": "concurrently \"webpack && webpack --watch\" \"npm run watchServer\" \"livereload ./public/js -w 1000 -d\"",
        "watchServer": "nodemon --ignore client --ignore public --ignore lib ./bin/www",
        "packageClient": "rimraf ./public/js && webpack",
        "lint": "eslint --ext .ts,.tsx ./client --color --fix",
        "makeDoc": "rimraf ./doc && typedoc --options typedoc.json",
        "webpack": "webpack",
        "debug": "nodemon --inspect ./bin/www"
    },
