  "scripts": {
    "watch": "node .tasks/watch.js",
    "commit:changes": "node ./.tasks/commit-changes.js",
    "start": "node ./dist/server/index.js",
    "package:client": "webpack",
    "build:server": "tsc --project tsconfig.json && copyfiles -u 1 \"server/public/**/*\" dist/server",
    "debug:server": "npm run build:server; ts-node ./server/index.ts -T --inspect --wait",
    "create-env": "node .tasks/create-dot-env.js",
    "postversion": "npm run typedoc && npm run eslint:fix && git add --all && git commit --amend",
    "prepare": "husky install"
  },
